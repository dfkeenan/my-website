Title: Blog posts
---   
@Html.Partial("../../_PostList.cshtml")
<nav class="mt-3" >
  <ul class="pagination justify-content-end">
    <li class="page-item @(Model.Bool(Keys.HasPreviousPage) ? "" : "disabled")">
      <a class="page-link" href="@(Context.GetLink(Model.Document(Keys.PreviousPage)))" tabindex="-1">Previous</a>
    </li>
 
    <li class="page-item @(Model.Bool(Keys.HasNextPage) ? "" : "disabled")">
      <a class="page-link" href="@(Context.GetLink(Model.Document(Keys.NextPage)))">Next</a>
    </li>
  </ul>
</nav>
